<?xml version="1.0" encoding="utf-8"?>
<policy priority="1000" target="CONTINUE">
  <short>Set Gateway - DMZ to HOST</short>
  <description>     Part of the policy set "gateway". Enables services commonly needed for a     gateway, e.g. dns, dhcp.   </description>
  <service name="dns"/>
  <service name="dhcp"/>
  <ingress-zone name="dmz"/>
  <egress-zone name="HOST"/>
  <disable/>
</policy>
